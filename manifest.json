{
    "manifest_version": 2,
    "name": "Wallpaper Fetcher",
    "version": "1.0",
    "description": "Fetches a background wallpaper using an API",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "permissions": ["storage", "activeTab"]
  }
  